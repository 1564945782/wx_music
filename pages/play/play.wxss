page{
  background: #9F9C9C;
}
view{
  justify-content: center;
  align-items: center;
}
.wrap{
  width: 100%;
  height: 300px;
  display: flex;
}
.child01{
  width: 450rpx;
  height: 450rpx;
  background: #bbb;
  border-radius: 50%;
  display: flex;
}
.child02{
  width: 370rpx;
  height: 370rpx;
  background: #4B4949;
  border-radius: 50%;
  display: flex;
}
.child03{
  width: 300rpx;
  height: 300rpx;
  background: #797979;
  border-radius: 50%;
  overflow: hidden;
}
.child03 image{
  width: 100%;
  height: 100%;
}
.content{
  display: flex;
  justify-content: space-around;
}
.content image{
  height: 100rpx;
  width: 100rpx;
}
